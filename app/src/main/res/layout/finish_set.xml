<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#B0BCAB">

    <EditText
        android:id="@+id/epointstxt5"
        android:layout_width="182dp"
        android:layout_height="48dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="48dp"
        android:layout_marginBottom="124dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/points_earned"
        android:textColor="#5A4947"
        app:layout_constraintBottom_toTopOf="@+id/Oh_no"
        app:layout_constraintEnd_toStartOf="@+id/imageButton5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/epoints5"
        android:layout_width="130dp"
        android:layout_height="52dp"
        android:layout_marginBottom="56dp"
        android:autofillHints=""
        android:ems="10"
        android:hint="@string/epoints"
        android:inputType="number"
        android:textColor="#5A4947"
        app:layout_constraintBottom_toTopOf="@+id/finishSetTxt"
        app:layout_constraintEnd_toStartOf="@+id/imageButton5"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/epointstxt5"
        app:layout_constraintVertical_bias="0.52" />

    <TextView
        android:id="@+id/finishSetTxt"
        android:layout_width="311dp"
        android:layout_height="137dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="208dp"
        android:text="Congrats! You have finished the set"
        android:textColor="#5A4947"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="131dp"
        android:layout_height="121dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="32dp"
        android:contentDescription="@string/back_home"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rsz_1brigham_s_bears" />

    <Button
        android:id="@+id/subjectSelectBut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginBottom="256dp"
        android:backgroundTint="#ffffff"
        android:text="Subject Selection"
        android:textColor="#5A4947"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/mainMenuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="176dp"
        android:backgroundTint="#ffffff"
        android:text="Main Menu"
        android:textColor="#5A4947"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>